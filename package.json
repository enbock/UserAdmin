{
  "name": "UserAdmin",
  "version": "1.0.0",
  "description": "React.js Demo and Learn-Project",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "start": "webpack-dev-server",
    "build": "webpack",
    "eslint": "eslint src & eslint tests"
  },
  "dependencies": {
    "ajv": "^5.2.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-loader": "^1.9.0",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/tests/_coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "^config$": "<rootDir>/config/test",
      ".*.less$": "<rootDir>/tests/less.js",
      ".*.vars$": "<rootDir>/tests/less.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/config/",
      "<rootDir>/tests/_coverage/",
      "/Bootstrap.js",
      "<rootDir>/tests/less.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  }
}
